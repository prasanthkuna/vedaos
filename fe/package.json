{
  "name": "veda-fe",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.4",
  "main": "expo-router/entry",
  "scripts": {
    "start": "bunx expo start",
    "android": "bunx expo run:android",
    "ios": "bunx expo run:ios",
    "web": "bunx expo start --web",
    "typecheck": "bunx tsc --noEmit",
    "e2e:maestro": "powershell -ExecutionPolicy Bypass -File ./scripts/run-maestro.ps1",
    "e2e": "bun run typecheck && bun run e2e:maestro"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.29",
    "@react-native-async-storage/async-storage": "2.2.0",
